{
	"name": "my-app",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:migrate:create": "drizzle-kit generate:sqlite --out db/migrations --schema db/src/schema.ts",
		"db:migrate:apply:local": "wrangler d1 migrations apply my-app-db_preview --local",
		"db:migrate:apply:prd": "wrangler d1 migrations apply my-app-db_preview"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-cloudflare-workers": "^1.2.1",
		"@sveltejs/kit": "^1.27.4",
		"@types/cookie": "^0.5.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"better-sqlite3": "^9.1.1",
		"drizzle-kit": "^0.20.6",
		"drizzle-orm": "^0.29.0",
		"prisma": "^5.6.0",
		"wrangler": "^3.17.1"
	},
	"type": "module",
	"dependencies": {
	}
}
